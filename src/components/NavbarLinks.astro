---
import { navbarLinks } from '../data/navbar-links';
---

{navbarLinks.map(navbarLinks => (
  <a
    href={`#${navbarLinks.id}`}
    class="hover:text-cyan-600 dark:hover:text-cyan-400 transition-colors"
    data-es={navbarLinks.es}
    data-en={navbarLinks.en}
  >
    {navbarLinks.es}
  </a>
))}
